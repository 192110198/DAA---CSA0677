#include<stdio.h>
void reverse(int n);
int main()
{
    int n,count=0;
    printf("Enter the number:");
    count++;
    scanf("%d",&n);
    count++;
    reverse(n);
    count++;
    return 0;
}
void reverse(int n)
{
    int count=0;
    int rev=0,rem;
    count++;
    count++;
    while(n!=0)
    {
        count++;
        rem=n%10;
        count++;
        rev=rev*10+rem;
        count++;
        n=n/10;
		count++;
	}
    count++;
    printf("\nreversed number is %d",rev);
    count++;
    printf("\n time complexity %d",count);
}
